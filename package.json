{
  "name": "cerberus-node-client",
  "version": "2.0.0",
  "description": "Node client for Cerberus Key Mgmt",
  "main": "index.js",
  "scripts": {
    "test": "npm run style && npm run test:unit:local",
    "test:unit:ci-report-coverage": "jest test/ --coverage --collectCoverageFrom=lib/**/*.js --collectCoverageFrom=index.js --collectCoverageFrom=!lib/sts.js | coveralls",
    "test:unit:local": "jest test/ --coverage --collectCoverageFrom=lib/**/*.js --collectCoverageFrom=index.js --collectCoverageFrom=!lib/sts.js",
    "test:integration": "jest integration/",
    "style": "standard \"index.js\" \"test/**/*.js\" | snazzy",
    "style:fix": "standard \"index.js\" \"test/**/*.js\" --fix",
    "generate-docs": "./scripts/generate-docs.sh",
    "deploy-docs": "gh-pages -d build/docs",
    "travis-deploy-github-pages": "gh-pages -r \"https://${GH_TOKEN}@github.com/yaorg/node-measured.git\" -d build/docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nike-Inc/cerberus-node-client.git"
  },
  "keywords": [
    "nike",
    "cerberus"
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "Tim Kye",
  "license": "Apache-2.0",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "aws-sdk": "^2.368.0",
    "crypto": "^1.0.1",
    "form-data": "^2.3.2",
    "request-micro": "^1.3.0",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "coveralls": "^3.0.2",
    "gh-pages": "^2.0.1",
    "ink-docstrap": "^1.3.2",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "nyc": "^11.0.2",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1"
  }
}
