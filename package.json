{
  "name": "cerberus-node-client",
  "version": "0.3.1",
  "description": "Node client for Cerberus Key Mgmt",
  "main": "index.js",
  "scripts": {
    "style": "standard \"index.js\" \"test/**/*.js\" | snazzy",
    "style:fix": "standard \"index.js\" \"test/**/*.js\" --fix",
    "test:unit": "blue-tape test/**/**.spec.js | ./node_modules/.bin/tap-spec",
    "test:coverage": "covert -c test/*.js",
    "test:coverage:error-only": "covert -c -q test/*.js",
    "test:lineread": "node test/lineread.manual.js",
    "test": "npm run style && npm run test:unit",
    "test:watch": "npm run test:unit -s & chokidar 'lib/**/*.js' 'index.js' 'test/**/*.spec.js' --polling -c \"npm run test:unit -s\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nike-Inc/cerberus-node-client.git"
  },
  "keywords": [
    "nike",
    "cerberus",
    "vault"
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "author": "Tim Kye",
  "license": "Apache-2.0",
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "request-micro": "^1.2.1",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.7.1",
    "babel-eslint": "^6.1.0",
    "blue-tape": "^0.2.0",
    "chokidar-cli": "^1.2.0",
    "covert": "^1.1.0",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1"
  }
}
